        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Updating the "Associated benefit" field of the existing benefit plan to the newly created benefit plan and setting "Associated Benefit" of previously associated benefit plan as empty.
 var benefitPlanObj = new BenefitPlan(current, previous);
    if (current.operation() == "update") {
        if (JSUtil.nil(current.getValue('associated_benefit'))) {
            benefitPlanObj.unassociatePreviousBenefit();
        } else {
            benefitPlanObj.associateCurrentBenefit();
            if (JSUtil.notNil(previous.getValue('associated_benefit'))) {
                benefitPlanObj.unassociatePreviousBenefit();
            }
        }
    } else {
        benefitPlanObj.associateCurrentBenefit();
    }

})(current, previous);]]></script>
