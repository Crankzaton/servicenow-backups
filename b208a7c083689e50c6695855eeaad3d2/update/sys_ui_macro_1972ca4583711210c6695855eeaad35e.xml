        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<g2:evaluate var="jvar_existflag">        
        var existflag = false;
		var data = current.x_938076_now_utils_audit_history;
		if (data!=''){existflag = true;}
        existflag;
</g2:evaluate>
<j2:if test="$[jvar_existflag == false]">		
<div>No History Available</div>
</j2:if>
<j2:if test="$[jvar_existflag == true]">
<style>
  th {
    background-color: #293e40;
	color:#ffffff;
  }

  tr:nth-child(even) {
    background-color: #80b6a1;
	color:#000000;
  }


</style>
<table style="text-align:center; border-collapse: collapse; width: 100%;">
  <tr>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Previous Priority	</th>
	<th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Current Priority</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Assigned To</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Channel</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Account</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Contract</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Contact</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Product</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Previous Updated Time(GMT)</th>
    <th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Resolved By</th>
	<th style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">Resolution Code</th>
  </tr>


  <g2:evaluate var="jvar_data" object="true">
	var unsorteddata=JSON.parse(current.x_938076_now_utils_audit_history);
	unsorteddata;	
  </g2:evaluate>


  <j2:forEach var="jvar_row" items="$[jvar_data]">
  <tr>
    <td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.previousPriority]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.currentPriority]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.assignedTo]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.channel]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.account]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.contract]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.contact]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.product]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.previousUpdatedTimeStamp]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.resolvedBy]</td>
	<td style="white-space: pre border: 1px solid #dddddd;  text-align: center;  padding: 8px;">$[jvar_row.resolutionCode]</td>
  </tr>
  </j2:forEach>
</table>
</j2:if>
