        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 if (!SNC.SpotlightUtil.removeDBView(SNC.SpotlightUtil.getFactsTable(current.indicator), current.sys_id)) {
  var dbView = current.database_view;
  var name = current.name;
  var message = gs.getMessage("Unable to remove database view with sys_id {0} for spotlight group '{1}'", [dbView, name]);
  if (!dbView)
   message = gs.getMessage("Unable to remove database view for {0}", [name]);
  gs.addErrorMessage(message);
  current.setAbortAction(true);
 } 
})(current, previous);]]></script>
