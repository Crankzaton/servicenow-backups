        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 var reserved_1 = gs.getProperty('com.snc.project.task.default_columns').split(',');
 var reserved_2 = gs.getProperty('com.snc.project.system.default_columns').split(',');
 reserved_1.push('start_date');
 reserved_1.push('end_date');
 
 if ( reserved_1.indexOf(current.getValue('json_column')) != -1 || 
     reserved_2.indexOf(current.getValue('json_column')) != -1 ) {
  current.setAbortAction(true);
  gs.addInfoMessage(gs.getMessage('Invalid Json Column name'));
  gs.addErrorMessage(gs.getMessage('Json column cannot be {0}, {1}', [reserved_1.join(','), reserved_2.join(',')])); 
 }
})(current, previous);]]></script>
