        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 if(gs.tableExists('pm_project_task') && current.project_phase){
  var projectTask = new GlideRecord('pm_project_task');
  if(projectTask.get(current.project_phase)&&projectTask.assignment_group)
   current.setValue('assignment_group',projectTask.assignment_group);
 }else if(current.product){
  var gr = new GlideRecord('m2m_product_group');
  gr.addQuery('product',current.product);
  gr.query();
  if(gr.next() && gr.getRowCount() === 1)
   current.setValue('assignment_group',gr.getValue('group'));
 }
})(current, previous);]]></script>
