        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

 var blockedByGr = new GlideRecord('sn_action_status_blocked_by');
 blockedByGr.addQuery('blocked_task', current.sys_id);
 blockedByGr.addNullQuery('unblocked_by');
 blockedByGr.addNullQuery('unblocked_on');
 blockedByGr.query();
 
 while(blockedByGr.next()){
   blockedByGr.setValue('unblocked_by', gs.getUserID());
   blockedByGr.setValue('unblocked_on', new GlideDateTime().getValue());
   blockedByGr.update();
 }
 current.action_status = "";
    current.needs_attention = false;

})(current, previous);]]></script>
