        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

 var gr = new GlideRecord('sn_imt_core_property');
 gr.addActiveQuery();
 if (current.getValue('sys_domain') !== 'global') {
  gr.addQuery('sys_domain', '!=', 'global');
 }
 if (!current.isNewRecord()) {
  gr.addQuery('sys_id', '!=', current.getUniqueValue());
 }
 gr.addQuery('existing_property', current.getValue('existing_property'))
  .addOrCondition('name', current.existing_property.name);
 gr.setLimit(1);
 gr.query();

 if (gr.next()) {
  gs.addErrorMessage(gs.getMessage(
   "Property '{0}' already has an active value for domain '{1}'",
   [
    current.existing_property.name,
    current.sys_domain.name || current.getValue('sys_domain'),
   ]
  ));
  current.setAbortAction(true);
 }

})(current, previous);]]></script>
