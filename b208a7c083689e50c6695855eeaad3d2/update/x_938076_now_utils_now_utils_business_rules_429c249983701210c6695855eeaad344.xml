        <script><![CDATA[/**
 * Modify the list of encryption types on the encryption context record.  If we're running a JVM before 1.5, 
 * 256-bit AES will not be available.
 */
function sys_encryption_context_typeGetChoices() {
   var version = gs.getProperty("system.java.version");
   answer.add("AES", gs.getMessage("AES 128-bit"));

   if (getMaxCipher("AES") >= 256)
      answer.add("AES256", gs.getMessage("AES 256-bit"));
}

function getMaxCipher(cipherType) {
var maxKeyLen = Packages.javax.crypto.Cipher.getMaxAllowedKeyLength(cipherType);
return maxKeyLen;
}]]></script>
