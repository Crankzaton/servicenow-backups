        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 var opLabel;
 var status;
 var gcl = new GlideChoiceList();
 
 if (current.service_status.changes()) {
  var choiceList = gcl.getChoiceList('cmdb_ci_service', 'service_status');
  opLabel = choiceList.getChoice('operational').getLabel();
  status = current.getDisplayValue('service_status');
 }
 else {
  opLabel = new global.SPMUtilsFactory().getUtils().getLifeCycleStageStatus(current,'catalog', 'operational');
  status = current.getDisplayValue('life_cycle_stage_status');
 }
 
 gs.addErrorMessage(gs.getMessage('The parent service is not in {0} state, this offering cannot be made {1}.', [opLabel, status]));
 current.setAbortAction(true);
})(current, previous);]]></script>
