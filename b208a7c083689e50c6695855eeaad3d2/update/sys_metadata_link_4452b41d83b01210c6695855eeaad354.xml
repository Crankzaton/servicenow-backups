        <payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_script_fix"&gt;
    &lt;sys_script_fix action="INSERT_OR_UPDATE"&gt;
        &lt;before&gt;false&lt;/before&gt;
        &lt;description&gt;//Working FInal Code ****** Do not edit&lt;/description&gt;
        &lt;name&gt;Final Code UI action&lt;/name&gt;
        &lt;record_for_rollback&gt;true&lt;/record_for_rollback&gt;
        &lt;script&gt;&lt;![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var metType = []; //Array of assessment sys ID to pass to UI action/HTMl click here
    var metName = []; // Array of Metric Type Display names for passing to HTML Code

    var survey_response = new GlideRecord('asmt_assessment_instance');
    survey_response.addQuery('trigger_id', current.parent.sys_id);
    survey_response.addQuery('state=complete'); // need to enable at last
    survey_response.addQuery('metric_type.display_value', 'Review Assessment').addOrCondition('metric_type.display_value', 'Review Intiiation Assessment').addOrCondition('metric_type.display_value', 'Demand Assessment')
    survey_response.addQuery('ORDERBYcreated_on');
    survey_response.query();
    while (survey_response.next()) {
        var asmtMet = survey_response.getValue('sys_id'); //Extracting assessment sys ID
        metType.push(asmtMet);

        var metTypeName = survey_response.metric_type.getDisplayValue(); //Extracting assessment name
        metName.push(metTypeName);

    }

    // gs.addInfoMessage(metType[0]);
    // gs.addInfoMessage(metType[1]);
    // gs.addInfoMessage(metType[2]);

    g_scratchpad.cmsID = metType[0];

    g_scratchpad.demandASMT = metType[0];
    g_scratchpad.reviewASMT = metType[1];
    g_scratchpad.reviewideaASMT = metType[2];

    var url1 = 'asmt_assessment_instance.do?sys_id=' + metType[0];
    var url2 = 'asmt_assessment_instance.do?sys_id=' + metType[1];
    var url3 = 'asmt_assessment_instance.do?sys_id=' + metType[2];

    gs.addInfoMessage("Please" + "  " + '&lt;a href="' + gs.getProperty('glide.servlet.uri') + url1 + '" target="_blank"&gt;Click here&lt;/a&gt;' + "  " + "to view" + " " + metName[0]);

    gs.addInfoMessage("Please" + "  " + '&lt;a href="' + gs.getProperty('glide.servlet.uri') + url2 + '" target="_blank"&gt;Click here&lt;/a&gt;' + "  " + "to view" + " " + metName[1]);

    gs.addInfoMessage("Please" + "  " + '&lt;a href="' + gs.getProperty('glide.servlet.uri') + url3 + '" target="_blank"&gt;Click here&lt;/a&gt;' + "  " + "to view" + " " + metName[2]);

    // add the form message, using strings and the link variable that was built above
    //gs.addInfoMessage('&lt;a href="' + gs.getProperty('glide.servlet.uri') + url + '"&gt;LINK&lt;/a&gt;');




})(current, previous);]]&gt;&lt;/script&gt;
