        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    if (current.override_approval_policy.changes()) {
        if (current.override_approval_policy == WSDConstants.OVERRIDE_APPROVAL_POLICY.always_required)
            current.require_approval = true;
        else
            current.require_approval = false;
    } else if (current.require_approval.changes()) {
        if (current.require_approval)
            current.override_approval_policy = WSDConstants.OVERRIDE_APPROVAL_POLICY.always_required;
        else
            current.override_approval_policy = WSDConstants.OVERRIDE_APPROVAL_POLICY.never_required;
    }

})(current, previous);]]></script>
