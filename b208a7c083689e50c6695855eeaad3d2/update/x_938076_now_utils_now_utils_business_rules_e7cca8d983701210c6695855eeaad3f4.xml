function updateScrumTaskRecord() {

  if (!current.story)
      current.story = current.parent;

  if (current.parent) {
    if (!current.product)
      current.product = current.parent.product;
    if (!current.assigned_to)
      current.assigned_to = current.parent.assigned_to;
 if (!current.assignment_group)
      current.assignment_group = current.parent.assignment_group;
  }

  if (current.story) {
    if (!current.product)
      current.product = current.story.product;
    if (!current.assigned_to)
      current.assigned_to = current.story.assigned_to;
 if (!current.assignment_group)
      current.assignment_group = current.story.assignment_group;
  }
}]]></script>
