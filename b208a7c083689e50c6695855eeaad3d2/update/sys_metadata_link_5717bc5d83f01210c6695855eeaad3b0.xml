        &lt;name&gt;alertTable&lt;/name&gt;
        &lt;script&gt;&lt;![CDATA[var alertTable = Class.create();
alertTable.prototype = Object.extendsObject(AbstractAjaxProcessor, {

    incDetails: function() {
        var parent_record = new GlideRecord('incident');
		var workNotes = '';
        var recID = this.getParameter('sysparm_recID');
        if (parent_record.get(recID)) {
            gs.addInfoMessage('hello');
            
            workNotes += '&lt;style&gt; table, th, td {border: 1px solid black;border-collapse: collapse;background-color:lightyellow;padding:2px;text-align: left;}&lt;/style&gt;&lt;table style="width:100%"&gt;';

            // Assuming assessment responses are stored in the "Question" field
            workNotes += '&lt;tr style="height: 50.4px;"&gt;&lt;td style="width: 500px; height: 15.4px;"&gt;&lt;strong&gt;' + parent_record.number + '&lt;/strong&gt;&lt;/td&gt;' + '&lt;td style="text-align: center; "width: 700px; height: 15.4px;""&gt;' + parent_record.assigned_to.getDisplayValue() + '&lt;/td&gt;';

            // End the table
            workNotes += '&lt;/table&gt;';

        }
        return workNotes;

    },
    type: 'alertTable'
});]]&gt;&lt;/script&gt;
