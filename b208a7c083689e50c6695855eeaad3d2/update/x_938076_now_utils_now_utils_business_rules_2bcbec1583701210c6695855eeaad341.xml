        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

    var gr = new GlideRecord(sn_ent.EnterpriseContentEAMUtils.BASE_CLASSES.ASSET_BASE_CLASS);
    query = "model_category.sys_id=" + current.sys_id;
    gr.addEncodedQuery(query);
    gr.query();

    // if aleast one eam asset is assigned to this category then do not allow edit of model category
    if (gr.getRowCount() >= 1) {
        gs.addErrorMessage(gs.getMessage("Assets are assigned to the model category therefore it cannot be edited."));
        global.ModelUtils.setAbortAction(current, true);
    }

})(current, previous);]]></script>
