        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 var wsdCampusSysIdToExclude = current.getValue('wsd_campus');
 var indoorCampusSysId = current.getValue('indoor_map_campus');
 var wsdCampusGr = new GlideRecord('sn_wsd_core_campus');
 wsdCampusGr.addQuery('sys_id', '!=', wsdCampusSysIdToExclude);
 wsdCampusGr.addQuery('indoor_map_campus', indoorCampusSysId);
 wsdCampusGr.query();

 if (wsdCampusGr.getRowCount() > 0) {
  gs.addErrorMessage(gs.getMessage('This Indoor Mapping Campus is already linked to another WSD campus'));
  current.setAbortAction(true);
 } else {
  current.setValue('state', 'clean');
 }
})(current, previous);]]></script>
