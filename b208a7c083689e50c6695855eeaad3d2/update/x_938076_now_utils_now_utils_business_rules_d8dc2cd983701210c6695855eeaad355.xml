        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 if(JSUtil.notNil(current.getValue("pm_console"))) {
  var entityTable = current.pm_console.table;
  if(JSUtil.notNil(entityTable)) {
   var entityTableRecord = new GlideRecord(entityTable);
   if(entityTableRecord.instanceOf("pm_project")) {
    gs.addErrorMessage(gs.getMessage("Sorting cannot be enabled on Project console"));
    current.sortable = false;
   }
  }
 }
})(current, previous);]]></script>
