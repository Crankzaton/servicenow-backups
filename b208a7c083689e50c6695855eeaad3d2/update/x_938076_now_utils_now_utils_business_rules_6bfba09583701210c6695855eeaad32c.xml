        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var newInternalName = current.getValue('internal_name').replace(/[^a-zA-Z0-9_]/g, '_');
    // Compact any multiple sequential underscores
    while (newInternalName.indexOf("__") != -1) {
        newInternalName = newInternalName.replace(/__/g, "_");
    }
    // Replace any trailing underscores
    while (newInternalName != '' && newInternalName.charAt(newInternalName.length - 1) == '_') {
        newInternalName = newInternalName.substring(0, newInternalName.length - 1);
    }
    current.setValue('internal_name', newInternalName);
})(current, previous);]]></script>
